<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mybatis.PostgreSQLMapper">

    <!-- Select a single user by ID -->
    <select id="getBodyByCode" resultType="java.lang.String" parameterType="String">
        SELECT body FROM finance.financedata WHERE param = #{code}
    </select>

</mapper>
